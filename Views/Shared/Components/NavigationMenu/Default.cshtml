@model IEnumerable<string>

<div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
    <ul class="navbar-nav flex-grow-1">
        <li class="nav-item ">
            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="">Home</a>
        </li>
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="About">About</a>
        </li>
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Product" asp-action="">All
                Products</a>
        </li>
        @foreach (string category in Model ?? Enumerable.Empty<string>())
        {
            <li class="nav-item text-dark">
                <a class="nav-link text-dark" asp-area="" asp-controller="Category" asp-action=""
                    asp-route-category="@category" asp-route-productPage="1">@category</a>
            </li>
        }
    </ul>
</div>